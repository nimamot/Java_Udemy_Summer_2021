
public class Human {
	
	
	
	//attributes
	String name;
	int age;
	int heightInInches;
	String eyeColor;
	
	public Human() {
		
	}
	
	// Methods
	public void speak() {
		System.out.println("Hello my name is " + name);
		System.out.println("I am " + heightInInches + " Inches");
		System.out.println("I am " + age  + " years old");
	}
	
	public void eat() {
		System.out.println("eating...");
		
	}
		
	public void walk() {
		System.out.println("Walking");
	}
	
	public void work() {
		System.out.println("Working");
		
	}
}
	
	
