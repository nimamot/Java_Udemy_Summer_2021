
public class Earth {
	
	public static void main(String args[]) {
		Human tom;
		tom = new Human();
		tom.name = "Tom Grey";
		tom.age = 5;
		tom.eyeColor = "brown";
		tom.heightInInches = 72;
		tom.speak();
		
		Human joe;
		joe = new Human();
		
		joe.name = "Joe Karav";
		joe.age = 21;
		joe.eyeColor = "Blue";
		joe.heightInInches = 140;
		
		joe.speak();
		
		
		
		
	}

}
